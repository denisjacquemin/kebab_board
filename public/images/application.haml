!!! Strict
%html
  %head
    %title nice-layout
    = stylesheet_link_tag 'wrapper', 'header', 'tab', 'global', 'awesome-button', 'table'
    = yield :css
    = javascript_include_tag :defaults
    = yield :javascript
  %body
    #center
      #header
        = render :partial => 'shared/links'
        %h1 
          = current_user.agency.name
        = render :partial => 'shared/tabs'
        .clear
      #wrapper
        = yield
        = render :partial => 'shared/footer'
        .clear
      .clear