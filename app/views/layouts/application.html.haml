!!! Strict
%html
  %head
    %title nice-layout
    = stylesheet_link_tag 'wrapper', 'header', 'tab', 'global', 'awesome-button', 'table'
    = yield :css
    = google_jquery :version => '1.4.0'
    = javascript_include_tag 'nested_form'
    = yield :javascript
  %body
    #center
      #header
        %h1 
          The kebab board
        = render :partial => 'shared/tabs'
        .clear
      #wrapper
        = yield
        = render :partial => 'shared/footer'
        .clear
      .clear